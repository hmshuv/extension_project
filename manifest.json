{
    "manifest_version": 2,
    "name": "URL Checker Extension",
    "version": "1.0",
    "description": "Checks if the current URL is Google.",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
   
    "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html",
      "default_title": "URL Checker"
    }
  }
  